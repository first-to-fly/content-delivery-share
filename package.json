{
  "name": "myawesomeproject",
  "version": "0.0.1",
  "private": true,
  "description": "Some description.",
  "main": "build",
  "files": [
    "build/*"
  ],
  "scripts": {
    "postinstall": "(patch-package || true) && echo",
    "ts-node": "TS_NODE_PROJECT=./tsconfig.test.json TS_NODE_TRANSPILE_ONLY=true node --require ts-node/register --require tsconfig-paths/register",
    "dev": "nodemon ./src/index.js",
    "build": "tsc --allowJs false --checkJs false --declaration && tsc",
    "start": "NODE_ENV=production NODE_PATH=./build node ./build",
    "lint:js": "eslint --ignore-pattern \"src/@nabstudio/**\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:ts": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint:css": "stylelint --allow-empty-input --ignore-pattern \"src/@nabstudio/**\" \"./src/**/*.{css,less,scss,sss}\"",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "test:mocha": "TS_NODE_PROJECT=./tsconfig.test.json mocha --require ts-node/register --require tsconfig-paths/register \"./test/**/*\"",
    "test:cypress": "./pipeline/test-cypress",
    "test": "npm run test:mocha && npm run test:cypress",
    "coverage": "nyc npm run test"
  },
  "repository": {},
  "license": "Apache",
  "dependencies": {
    "@types/node": "^12.12.22",
    "deepcopy": "^2.0.0",
    "patch-package": "^6.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "mocha": "^6.2.2",
    "mock-require": "^3.0.3",
    "nock": "^11.7.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "sinon": "^8.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
