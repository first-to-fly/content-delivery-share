{
  "name": "myawesomeproject",
  "version": "0.0.1",
  "private": true,
  "description": "Some description.",
  "author": {
    "name": "NAB Studio"
  },
  "main": "build",
  "files": [
    "build/*"
  ],
  "scripts": {
    "postinstall": "(patch-package || true) && echo",
    "ts-node": "TS_NODE_PROJECT=./tsconfig.json TS_NODE_TRANSPILE_ONLY=true node --inspect=localhost:${NODE_DEBUG_PORT:-9229} --require ts-node/register --require tsconfig-paths/register",
    "dev:nodemon": "nodemon ./src/index.js",
    "dev": "npm run dev:nodemon",
    "build:tsc-declaration": "tsc --allowJs true --checkJs false --declaration",
    "build:tsc": "tsc",
    "build": "concurrently --kill-others-on-fail --names \"tsc-declaration,tsc\" \"npm run build:tsc-declaration\" \"npm run build:tsc\"",
    "start:node": "NODE_PATH=./build node ./build",
    "start": "npm run start:node",
    "lint:eslint": "eslint --ignore-pattern \"submodules/**\" --ignore-pattern \"src/@nabstudio/**\" --ignore-pattern \"example/**\" \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:tsc": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint:stylelint": "stylelint --allow-empty-input --ignore-pattern \"submodules/**\" --ignore-pattern \"src/@nabstudio/**\" \"./**/*.{css,less,scss,sss}\"",
    "lint": "concurrently --kill-others-on-fail --names \"eslint,tsc,stylelint\" \"npm run lint:eslint\" \"npm run lint:tsc\" \"npm run lint:stylelint\"",
    "test:mocha": "TS_NODE_PROJECT=./tsconfig.test.json mocha --require ts-node/register --require tsconfig-paths/register \"./test/**/*\"",
    "test:cypress": "npx cypress run",
    "test": "concurrently --kill-others-on-fail --names \"mocha,cypress\" \"npm run test:mocha\" \"npm run test:cypress\""
  },
  "repository": {},
  "license": "Apache",
  "dependencies": {
    "@types/node": "^12.0",
    "deepcopy": "^2.0.0",
    "patch-package": "^6.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unused-imports": "^0.1.3",
    "json-schema": "^0.2.5",
    "mocha": "^7.1.2",
    "mock-require": "^3.0.3",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "sinon": "^9.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2",
    "wait-on": "^5.0.0"
  }
}