{
  "name": "myawesomeproject",
  "version": "0.0.1",
  "private": true,
  "description": "Some description.",
  "author": {
    "name": "NAB Studio"
  },
  "main": "build",
  "files": [
    "build/*"
  ],
  "scripts": {
    "postinstall": "(npx patch-package || true) && echo",
    "ts-node": "TS_NODE_PROJECT=./tsconfig.json TS_NODE_TRANSPILE_ONLY=true node --inspect=localhost:${NODE_DEBUG_PORT:-9229} --require ts-node/register --require tsconfig-paths/register",
    "dev:nodemon": "npx nodemon ./src/index.js",
    "dev": "npm run dev:nodemon",
    "build:tsc-declaration": "tsc --allowJs true --checkJs false --declaration",
    "build:tsc": "tsc",
    "build": "npx concurrently --kill-others-on-fail --names \"tsc-declaration,tsc\" \"npm run build:tsc-declaration\" \"npm run build:tsc\"",
    "start:node": "NODE_PATH=./build node ./build",
    "start": "npm run start:node",
    "lint:eslint": "eslint --ignore-pattern \"submodules/**\" --ignore-pattern \"src/@nabstudio/**\" \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:tsc": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint:stylelint": "stylelint --allow-empty-input --ignore-pattern \"submodules/**\" --ignore-pattern \"src/@nabstudio/**\" \"./**/*.{css,less,scss,sss}\"",
    "lint": "npx concurrently --kill-others-on-fail --names \"eslint,tsc\" \"npm run lint:eslint\" \"npm run lint:tsc\"",
    "test:mocha": "TS_NODE_PROJECT=./tsconfig.test.json mocha --require ts-node/register --require tsconfig-paths/register \"./test/**/*\"",
    "test:cypress": "npx cypress run",
    "test": "npx concurrently --kill-others-on-fail --names \"mocha,cypress\" \"npm run test:mocha\" \"npm run test:cypress\""
  },
  "repository": {},
  "license": "Apache",
  "dependencies": {
    "deepcopy": "^2.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "json-schema": "^0.3.0",
    "mocha": "^8.3.0",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
