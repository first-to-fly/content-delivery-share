{
  "name": "myproject",
  "version": "0.0.1",
  "private": true,
  "description": "Some description.",
  "main": "./build",
  "files": [
    "./build/**/*"
  ],
  "scripts": {
    "postinstall": "patch-package && npm uninstall --no-save patch-package && echo",
    "dev": "nodemon ./src/index.js",
    "build": "tsc && tsc --allowJs false --checkJs false --declaration --outDir ./build-declaration && ([ ! -d ./build-declaration ] || (cp -R ./build-declaration/ ./build/ && rm -rf ./build-declaration))",
    "start": "NODE_ENV=production NODE_PATH=./build node ./build",
    "lint:js": "eslint --ignore-pattern \"src/@nabstudio/**\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:ts": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint:css": "stylelint --allow-empty-input --ignore-pattern \"src/@nabstudio/**\" \"./src/**/*.{css,less,scss,sss}\"",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "test:mocha": "mocha --require ts-node/register \"./test/**/*\"",
    "test:cypress": "./pipeline/test-cypress",
    "test": "npm run test:mocha && npm run test:cypress",
    "coverage": "nyc npm run test"
  },
  "repository": {},
  "license": "Apache",
  "dependencies": {
    "@types/node": "^12.7.5",
    "deepcopy": "^2.0.0",
    "patch-package": "^6.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "mocha": "^6.2.0",
    "mock-require": "^3.0.3",
    "nock": "^11.3.5",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "sinon": "^7.4.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.3"
  }
}
