{
  "name": "myproject",
  "version": "0.0.1",
  "private": true,
  "description": "Some description.",
  "main": "./build",
  "scripts": {
    "postinstall": "patch-package && npm uninstall --no-save patch-package && ([ ! -f ./pipeline/build ] || ! command -v tsc || ./pipeline/build) && echo",
    "dev": "TS_NODE_PROJECT=./tsconfig.json TS_NODE_TRANSPILE_ONLY=true node --inspect=localhost:9229 --require ts-node/register --require tsconfig-paths/register ./src",
    "build": "tsc",
    "start": "NODE_ENV=production node ./build",
    "lint:js": "eslint --ignore-pattern \"src/@nabstudio/**\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:ts": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint:css": "stylelint --allow-empty-input --ignore-pattern \"src/@nabstudio/**\" \"./src/**/*.{css,less,scss,sss}\"",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "test:mocha": "mocha --require ts-node/register \"./test/**/*\" || true",
    "test:cypress": "./pipeline/test-cypress",
    "test": "npm run test:mocha && npm run test:cypress",
    "coverage": "nyc npm run test"
  },
  "repository": {},
  "license": "Apache",
  "dependencies": {
    "@types/node": "^12.7.5",
    "deepcopy": "^2.0.0",
    "patch-package": "^6.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "mocha": "^6.1.4",
    "mock-require": "^3.0.3",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "sinon": "^7.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.2"
  }
}
