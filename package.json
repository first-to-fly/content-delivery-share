{
  "name": "myawesomeproject",
  "version": "0.0.1",
  "private": true,
  "description": "Some description.",
  "author": {
    "name": "NAB Studio"
  },
  "main": "build",
  "files": [
    "build/*"
  ],
  "scripts": {
    "postinstall": "npm_config_yes=true npx patch-package && echo",
    "ts-node": "TS_NODE_PROJECT=./tsconfig.json TS_NODE_TRANSPILE_ONLY=true node --inspect=localhost:${NODE_DEBUG_PORT:-9229} --require ts-node/register --require tsconfig-paths/register",
    "dev:nodemon": "nodemon ./src/index.ts",
    "dev": "npm run dev:nodemon",
    "build:tsc-declaration": "tsc --allowJs true --checkJs false --declaration",
    "build:tsc": "tsc",
    "build:node": "concurrently --kill-others-on-fail --names \"tsc-declaration,tsc\" --prefix-colors \"blue,cyan\" \"npm run build:tsc-declaration\" \"npm run build:tsc\"",
    "build": "npm run build:node",
    "postbuild": "",
    "start:node": "NODE_PATH=./build node ./build",
    "start": "npm run start:node",
    "lint:eslint": "eslint --ignore-pattern \"submodules/**\" \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:tsc": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint:madge": "madge --circular --no-spinner $(find ./src -type f | grep '\\.\\(ts\\|tsx\\|js\\|jsx\\|mjs\\)$' | paste -sd ' ' -)",
    "lint:stylelint": "stylelint --allow-empty-input ./**/*.{css,less,scss,sss}",
    "lint:depcheck": "npm_config_yes=true npx depcheck",
    "lint:unused-exports": "npm_config_yes=true npx ts-unused-exports ./tsconfig.json",
    "lint": "concurrently --kill-others-on-fail --names \"eslint,tsc,madge,stylelint\" --prefix-colors \"blue,cyan,magenta,gray,yellow,green\" \"npm run lint:eslint\" \"npm run lint:tsc\" \"npm run lint:madge\" \"npm run lint:stylelint\"",
    "test:mocha": "TS_NODE_PROJECT=./tsconfig.test.json mocha --require ts-node/register --require tsconfig-paths/register --timeout 300000 ./test/**/*.test.ts",
    "test:cypress": "cypress install && cypress run",
    "test": "concurrently --kill-others-on-fail --names \"mocha,cypress\" --prefix-colors \"magenta,gray\" \"npm run test:mocha\" \"npm run test:cypress\""
  },
  "repository": {},
  "license": "Apache",
  "dependencies": {
    "lodash.clonedeep": "4.5.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.9",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/mocha": "10.0.0",
    "@types/node": "18.11.7",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "concurrently": "7.5.0",
    "cypress": "10.11.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "madge": "5.0.1",
    "mocha": "10.1.0",
    "nodemon": "2.0.20",
    "prettier-eslint": "15.0.1",
    "prettier-eslint-cli": "7.1.0",
    "prettier-stylelint": "0.4.2",
    "stylelint": "14.14.0",
    "stylelint-config-standard-scss": "6.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typed-scss-modules": "7.0.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4"
}
