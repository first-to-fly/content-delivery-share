#!/usr/bin/env bash

# shellcheck disable=SC1090
source "${BASH_SOURCE[0]%/*}/../lib/bash/core.sh"

function main() {

  echo -e "${OK_COLOR}==> Cleaning...${NO_COLOR}"

  (
    set -x
    rm -rf "./build"
    rm -rf "./node_modules"
    rm -rf "./.nyc_output"
  )

}

main "$@" ||
  catch "$?" &&
  finally
